<?xml version="1.0" encoding="UTF-8"?>
<!-- Space group info XML file -->
<sg-xml>
  <space-group>Pnc2</space-group>
  <sg-num>30</sg-num>
  <hm-symbol>P n c 2</hm-symbol>
  <bravais>Orthorhombic</bravais>
  <settings num="6">
    <set name="P_n_c_2" x="a" y="b" z="c">
    </set>
    <set name="P_c_n_2" x="b" y="a" z="-c">
    </set>
    <set name="P_2_n_a" x="c" y="a" z="b">
    </set>
    <set name="P_2_a_n" x="-c" y="b" z="a">
    </set>
    <set name="P_b_2_n" x="b" y="c" z="a">
    </set>
    <set name="P_n_2_b" x="a" y="-c" z="b">
    </set>
  </settings>
  <wyckoff num="3">
    <wyck id="1" mul="4" let="c" site="1">
      <pos x="x" y="y" z="z"/>
      <pos x="-x" y="-y" z="z"/>
      <pos x="x" y="-y+1/2" z="z+1/2"/>
      <pos x="-x" y="y+1/2" z="z+1/2"/>
    </wyck>
    <wyck id="2" mul="2" let="b" site="..2">
      <pos x="1/2" y="0" z="z"/>
      <pos x="1/2" y="1/2" z="z+1/2"/>
    </wyck>
    <wyck id="3" mul="2" let="a" site="..2">
      <pos x="0" y="0" z="z"/>
      <pos x="0" y="1/2" z="z+1/2"/>
    </wyck>
  </wyckoff>
</sg-xml>
